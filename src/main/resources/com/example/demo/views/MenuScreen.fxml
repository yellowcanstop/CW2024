<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="com.example.demo.controller.MenuController">
    <children>
        <Label layoutX="126.0" layoutY="120" text="Welcome to Sky Battle!" />
        <Button fx:id="button1" layoutX="126.0" layoutY="90" onAction="#startGame" text="Start game!" />
        <Button fx:id="button2" layoutX="126.0" layoutY="90" onAction="#exitGame" text="Exit" />
    </children>
</AnchorPane>